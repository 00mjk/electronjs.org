  created: '2013-06-06T20:24:53.571Z',
  modified: '2016-11-18T13:49:34.655Z',
  lastPublisher: { name: 'jongleberry', email: 'jonathanrichardong@gmail.com' },
  owners: 
   [ { name: 'tjholowaychuk', email: 'tj@vision-media.ca' },
