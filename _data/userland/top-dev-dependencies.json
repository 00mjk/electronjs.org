     { number: '3.0.0', date: '2015-07-01T20:47:06.834Z' } ],
  created: '2011-05-14T07:38:58.395Z',
  modified: '2016-11-02T18:59:33.006Z',
  lastPublisher: { name: 'ljharb', email: 'ljharb@gmail.com' },
  owners: 
