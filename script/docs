#!/usr/bin/env node

const path = require('path')
const fetchDocs = require('electron-docs')
const Doc = require('../lib/doc')
const versions = require(path.join(__dirname, '../_data/versions.json')).map(v => v.version)

fetchDocs(versions[0])
  .then(docs => {
    docs.map(doc => new Doc(doc))
  })
  .catch(error => {
    throw error
  })
